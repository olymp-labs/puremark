# Pre-fill database with initial bookmarks (top 100 most visited websites without 'Adult Content' category)
# Set to 'true' to populate database on first startup (default: false)
DB_PREFILL=true

# Allow exporting the database from the Settings dialog
# Set to 'true' to enable the Export button (default: true)
DB_ALLOW_EXPORT=true

# Allow importing the database from the Settings dialog
# Set to 'true' to enable the Import button (default: true)
DB_ALLOW_IMPORT=true
